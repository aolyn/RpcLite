{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "copyToOutput": [
      "rpclite.config.json"
    ]
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "version": "1.0.0",
      "type": "platform"
    },
    "RpcLite.AspNetCore": "1.0.1-*",
    "RpcLite.Resolvers": "1.0.0-*",
    "RpcLiteTestService.Contract": "1.0.0-*",
    "ServiceRegistry.Contract": "1.0.0-*",
    "System.Net.Http": "4.1.0"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50"
    }
  },

  "publishOptions": {
    "include": [
      "wwwroot",
      "web.config",
      "rpclite.config.json"
    ]
  }

}
